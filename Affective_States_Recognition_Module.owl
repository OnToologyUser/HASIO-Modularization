    <!-- http://imash.leeds.ac.uk/ontologies/WNAffect/WNAffect.owl#Mental-State -->

    <owl:Class rdf:about="http://imash.leeds.ac.uk/ontologies/WNAffect/WNAffect.owl#Mental-State">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment>A mental state is a state of mind</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State -->

    <owl:Class rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State">
        <rdfs:subClassOf rdf:resource="http://imash.leeds.ac.uk/ontologies/WNAffect/WNAffect.owl#Mental-State"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isDetectedFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State_Recognition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <ace_lexicon:CN_pl>Affective_stateses</ace_lexicon:CN_pl>
        <ace_lexicon:CN_sg>Affective_states</ace_lexicon:CN_sg>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State_Recognition -->

    <owl:Class rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State_Recognition">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isSourceToDetect"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Ways to detect or recognize Affective States</rdfs:comment>
    </owl:Class>
    


