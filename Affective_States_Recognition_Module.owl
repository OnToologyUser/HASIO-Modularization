    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isDetectedFrom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isDetectedFrom">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isSourceToDetect"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Mood"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Sentiment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Emotion_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Mood_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Sentiment_Recognition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isSourceToDetect -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#isSourceToDetect">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Emotion_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Mood_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Sentiment_Recognition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Affective_State"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Mood"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASIO#Sentiment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


